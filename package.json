{
  "name": "objectel",
  "version": "0.0.0",
  "description": "monorepo of objectel",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build:examples": "lerna run --scope @examples/* build --stream",
    "build:packages:development": "lerna run --scope @objectel/* build:development --stream",
    "build:packages:production": "lerna run --scope @objectel/* build:production --stream",
    "build": "lerna run build --stream",
    "clear": "lerna run clear --stream"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/objectel/objectel.git"
  },
  "author": "ENvironmentSet <herryseo1@gmail.com> (https://github.com/ENvironmentSet)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/objectel/objectel/issues"
  },
  "homepage": "https://github.com/objectel/objectel#readme",
  "devDependencies": {
    "lerna": "^3.10.5"
  }
}
